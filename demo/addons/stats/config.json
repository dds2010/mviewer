{
  "js": ["js/stats.js", "js/jquery.easyDrag.min.js"],
  "css": "css/stats.css",
  "html": "stats.html?v=2",
  "target": "page-content-wrapper",
  "options": {
    "mviewer":{
      "stats":{
        "tooltipPosition": "bottom-left",
        "title": "Stats sur le JDD Structures",
        "open": true,
        "style": {
          "border": "1px #2e5367 solid",
          "background": "#2e5367",
          "text": "white",
          "colorButton": "#2e5367"
        },
        "stats": [
          {
            "type": "stat",
            "layerId": "structures",
            "field": "",
            "template":"<span class='structcss'>Structures :<br> {x} Objets</span>",
            "operator": "COUNT",
            "__operator": "SUM,MAX,MEAN,COUNT"
          },
          {
            "type": "stat",
            "layerId": "structures",
            "field": "cp",
            "template":"Moyenne : {x} ",
            "operator": "MEAN",
            "__operator": "SUM,MAX,MEAN,COUNT"
          },
          {
            "type": "stat",
            "layerId": "structures",
            "field": "cp",
            "template":"MIN : {x} ",
            "operator": "MIN",
            "__operator": "SUM,MAX,MEAN,COUNT"
          },
          {
            "type": "stat",
            "layerId": "structures",
            "field": "cp",
            "template":"MAX : {x} ",
            "operator": "MAX",
            "__operator": "SUM,MAX,MEAN,COUNT"
          }
        ]
      }
    }
  }
}
